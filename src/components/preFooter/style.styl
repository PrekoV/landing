.PreFooter
    height 100%
    width 100%
    .section10_wrapper
        display flex
        justify-content space-between
        margin: 0 auto 50px auto
        max-width: 1170px
        @media screen and (max-width 780px)
            flex-direction: column
        .list
            width: 23%
            margin: 0 auto
            @media screen and (max-width 780px)
                width 90%
            .title
                text-align: left
                border-bottom: 1px dotted #000
                border-top: 1px dotted #000
                padding: 12px 0
                margin: 32px 0 14px 0
                color: #000
                font-size: 20px
            ul
                list-style none
                font-size: 15px
                color: #333
                line-height: 30px
                li:hover
                    color #ce4624